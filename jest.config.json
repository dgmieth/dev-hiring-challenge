{
    "collectCoverage":true,
    "collectCoverageFrom": [
        "**/app/*.js",
        "**/app/*/*.js",
        "!**/app/server.js",
        "!**/app/*/servicesCtrl.js",
        "!**app/test/**",
        "!**/node_modules/**"
    ],
    "coverageThreshold": {
        "global": {
            "branches": 100,
            "functions": 100,
            "lines":100,
            "statements":100
        }
    }
}